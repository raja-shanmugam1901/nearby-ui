{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "max-len": [
            "error",
            {
                "code": 200
            }
        ],
        "import/no-unresolved": 0,
        "linebreak-style": "off"
    },
    "workingDirectories": [
        "./src"
    ]
}